# MongoDB Connection String from MongoDB Atlas
# IMPORTANT: Replace the placeholder with your actual connection string on your local computer
MONGO_URI=mongodb+srv://<username>:<password>@yourcluster.mongodb.net/yourDatabaseName?retryWrites=true&w=majority

# Port for the server
PORT=5001

# Razorpay API Keys
# IMPORTANT: Replace these with your real keys from the Razorpay dashboard on your local computer
RAZORPAY_KEY_ID=YOUR_KEY_ID_HERE
RAZORPAY_KEY_SECRET=YOUR_KEY_SECRET_HERE
